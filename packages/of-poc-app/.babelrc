// following plugins are installed:
//  - @babel/runtime:
//  - @babel/plugin-transform-runtime:
{
    "presets": [
        "@babel/preset-env",
        [
            "@babel/preset-react",
            {
                "runtime": "automatic"
            }
        ],
        "@babel/preset-typescript"
    ],
    "plugins": [
        [
            // allows to use async-await feature in react apps
            "@babel/plugin-transform-runtime",
            {
                "regenerator": true
            }
        ]
    ]
}

{
    "licenseKey": "your-license-here",
    "runtime": {
        "arguments": "--v=1 --inspect",
        "version": "stable"
    },
    "startup_app": {
        "name": "TradeView"
    },
    "platform": {
        "uuid": "platform_customization_local",
        "title": "My App",
        "autoShow": true,
        "permissions": {
            "ExternalWindow": {
                "wrap": true
            },
            "System": {
                "getAllExternalWindows": true,
                "launchExternalProcess": true,
                "readRegistryValue": false,
                "terminateExternalProcess": true
            }
        },
        "defaultWindowOptions": {
            "contextMenuSettings": {
                "reload": true,
                "devtools": true,
                "enable": true
            },
            "autoShow": true,
            "showTaskbarIcon": true,
            "defaultWidth": 900,
            "defaultHeight": 720,
            "defaultLeft": 0,
            "defaultTop": 0,
            "saveWindowState": false,
            "backgroundThrottling": true,
            "minHeight": 445,
            "minWidth": 354,
            "experimental": {
                "analytics": false
            }
        },
        "defaultViewOptions": {
            "experimental": {
                "childWindows": true,
                "analytics": false
            }
        },
        "commands": [
            {
                "command": "stack.nextTab",
                "keys": "Ctrl+Tab"
            }
        ]
    },
    "snapshot": {
        "windows": [
            {
                "name": "Seed Window",
                "layout": {
                    "content": [
                        {
                            "type": "view",
                            "id": "no-drop-target",
                            "content": []
                        }
                    ]
                }
            }
        ]
    }
}

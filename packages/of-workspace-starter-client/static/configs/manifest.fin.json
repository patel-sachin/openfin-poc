{
    "$schema": "https://json.schemastore.org/openfin.json",
    "devtools_port": 9090,
    "licenseKey": "your-license-here",
    "runtime": {
        "arguments": "--v=1 --inspect",
        "version": "23.96.68.*"
    },
    "startup_app": {
        "name": "workspace-starter-how-to-customize-workspace"
    },
